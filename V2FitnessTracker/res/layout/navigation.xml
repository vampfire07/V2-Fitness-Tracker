<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!-- Buttons do not have an onClick value set. It will be set in Java code. -->
   	<Button android:id="@+id/navigation_home"
   	    android:layout_width="wrap_content"
   	    android:layout_height="50dp"
   	    android:layout_weight="1"
   	    android:background="@drawable/home"/>
   	<Button android:id="@+id/navigation_exercise"
   	    android:layout_width="wrap_content"
   	    android:layout_height="50dp"
   	    android:layout_weight="1"
   	    android:background="@drawable/exercises"/>
   	<Button android:id="@+id/navigation_nutrition"
   	    android:layout_width="wrap_content"
   	    android:layout_height="50dp"
   	    android:layout_weight="1"
		android:background="@drawable/nutrition"/>
   	<Button android:id="@+id/navigation_journal"
   	    android:layout_width="wrap_content"
   	    android:layout_height="50dp"
   	    android:layout_weight="1"
   	    android:background="@drawable/journal"/>
   	<Button android:id="@+id/navigation_logout"
   	    android:layout_width="wrap_content"
   	    android:layout_height="50dp"
   	    android:layout_weight="1"
   	    android:background="@drawable/logout"/>
</LinearLayout>